<h1>Listing pdf_reports</h1>

<table>
  <tr>
    <th>Report Id</th>
    <th>Dr list</th>
    <th>Status</th>
    <th>Job</th>
    <th>Blob</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pdf_reports.each do |pdf_report| %>
  <tr>
    <td><%= pdf_report.id %></td>
    <td><%= pdf_report.dr_list %></td>
    <td><%= pdf_report.status %></td>
    <td><%= pdf_report.delayed_job_id %></td>
    <td><%= link_to 'Download', download_pdf_report_path(pdf_report) %></td>
    <td><%= link_to 'Show', pdf_report %></td>
    <td><%= link_to 'Edit', edit_pdf_report_path(pdf_report) %></td>
    <td><%= link_to 'Destroy', pdf_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Pdf report', new_pdf_report_path %>
